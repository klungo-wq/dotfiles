[core]
    editor = nvim
    ignorecase = false
[user]
	email = []
	name = []
[color]
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto
    ui = auto
[alias]
    st = status
    ci = commit
    ca = commit -a
    a = "!git status --short | peco | awk '{print $2}' | xargs git add"
    ps = "!git push origin $(git rev-parse --abbrev-ref HEAD)"
    pl = "!git pull origin $(git rev-parse --abbrev-ref HEAD)"
    open = "!hub browse"
